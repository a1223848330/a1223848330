              <Options moHideChildren="1"/>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>[ENABLE]

aobscanmodule(INJECT,gamedll_x64_rwdi.dll,F3 0F 11 87 B8 00 00 00 48 8B 01 FF 50 38) // should be unique
alloc(newmem,$1000,"gamedll_x64_rwdi.dll"+2B49B0)
alloc(FOV,20)
label(code)
label(return)
registersymbol(FOV)
