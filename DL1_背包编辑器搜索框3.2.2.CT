ChatWindow.show()
ChatWindow_OutputWindow.clear()
ChatFrequency=0.5
local lastTime=os.clock()

function readWideString(StrAddr)
  local CheckStr=readSmallInteger(StrAddr)
  local str={}
  local i=0
  while(CheckStr~=0)
  do
  CheckStr=readSmallInteger(StrAddr+i-1)
  str[i+1]=readBytes(StrAddr+i)
  i=i+1
  end

  local WideString=byteTableToWideString(str)
  return WideString
end

function writeWideString(WideStringAddr,WideString)
  function table_leng(t)
  local leng=0
  for k, v in pairs(t) do
    leng=leng+1
  end
  return leng;
  end
  local WideStringTable=wideStringToByteTable(WideString)
  for i=1,table_leng(WideStringTable) do
      writeBytes(WideStringAddr+i-1,WideStringTable[i])
  end
  writeSmallInteger(WideStringAddr+table_leng(WideStringTable),0x0000)
end

function ChatWindow_InputWindowChange(sender)
writeWideString(getAddress("MessageText"),ChatWindow_InputWindow.Lines.text)
end

function ChatWindow_InputWindowKeyDown(sender, key) local getTime=os.clock() if key==13 and (getTime-lastTime)&gt;=ChatFrequency and
    readSmallInteger("MessageText")~=0 then writeWideString(getAddress("MessageText"),ChatWindow_InputWindow.Lines.text) autoAssemble(SendMessageCallEnable) ChatWindow_InputWindow.clear() lastTime = getTime end end function ChatWindow_ButtonSendClick(sender) local getTime=os.clock() if
    (getTime-lastTime)&gt;=ChatFrequency and readSmallInteger("MessageText")~=0 then writeWideString(getAddress("MessageText"),ChatWindow_InputWindow.Lines.text) autoAssemble(SendMessageCallEnable) ChatWindow_InputWindow.clear() lastTime = getTime
    end
end

