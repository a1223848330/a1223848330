     if ModCheckNum&gt;=4 then if ModIndex_4 == 0 then writeQword(readQword(Weapon.Address+0x88)+0x18,0)
                                               else writeQword(readQword(Weapon.Address+0x88)+0x18,ModTable[ModIndex_4].Address) end
