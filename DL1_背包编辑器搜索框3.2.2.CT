                   else writeQword(Weapon.Address+0x78,ModTable[ModIndex].Address) end

     if ModCheckNum&gt;=1 then if ModIndex_1 == 0 then writeQword(readQword(Weapon.Address+0x88),0)
                                               else writeQword(readQword(Weapon.Address+0x88),ModTable[ModIndex_1].Address) end

