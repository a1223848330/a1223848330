if ChatWindow_ThemeColor.ItemIndex==1 then
ChatWindow.color=0xD6E4B6
ChatWindow_InputWindow.color=0xF9FBF4
ChatWindow_OutputWindow.color=0xE3EDCD
else
if ChatWindow_ThemeColor.ItemIndex==2 then
ChatWindow.color=0xA8BDF0
ChatWindow_InputWindow.color=0xcbe8f3
ChatWindow_OutputWindow.color=0xb4c6f2
else
ChatWindow.color=clDefault
ChatWindow_InputWindow.color=clDefault
ChatWindow_OutputWindow.color=clDefault
end
end
end

function ChatWindow_TransitChange(sender)
HudMessageElement=ChatWindow_Transit.text
ChatWindow_OutputWindow.append(readWideString(readQword(readQword(readQword(readQword(HudMessageElement+0x38)+0x18)+0x378)+0x30)))
end

function RefreshMessage()
  if readQword("getNewHudMessageElement") then
  if readQword("getNewHudMessageElement")~=0 then
     local NewHookPoint=readQword("getNewHudMessageElement")
    if NewHookPoint ~= OldHookPoint then
      local getTime=os.date("%H:%M:%S")
      ChatWindow_OutputWindow.append("["..getTime.."] "..readWideString(readQword(readQword(readQword(readQword(readQword("getNewHudMessageElement")+0x38)+0x18)+0x378)+0x30)))
      OldHookPoint=NewHookPoint
    end
  end
  end
end

