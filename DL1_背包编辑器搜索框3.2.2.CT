--读Unicode
function readWideString(StrAddr)
  local CheckStr=readSmallInteger(StrAddr)
  local str={}
  local i=0
  while(CheckStr~=0)
  do
  CheckStr=readSmallInteger(StrAddr+i-1)
  str[i+1]=readBytes(StrAddr+i)
  i=i+1
  end

  local WideString=byteTableToWideString(str)
  return WideString
end

--写Unicode
function writeWideString(WideStringAddr,WideString)
  function table_leng(t)
  local leng=0
  for k, v in pairs(t) do
    leng=leng+1
  end
  return leng;
  end
  local WideStringTable=wideStringToByteTable(WideString)
  for i=1,table_leng(WideStringTable) do
      writeBytes(WideStringAddr+i-1,WideStringTable[i])
  end
  writeSmallInteger(WideStringAddr+table_leng(WideStringTable),0x0000)
end

--功能
function ChineseTextInputShow()
  ChineseTextInput.show()

  WindowText=readWideString(getAddress("text"))
  ChineseTextInput_InputWindow.Text=WindowText

  --当改变时
  function ChineseTextInput_InputWindowChange(sender)
    WindowTextInput=ChineseTextInput_InputWindow.Text
    writeWideString(getAddress("text"),WindowTextInput)
  end
end

function ChineseTextInputHide()
  ChineseTextInput.close()
end




